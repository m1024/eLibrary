@model IEnumerable<eLibrary.Models.Series>

<div id="">

    @if (@Model != null && @Model.Count()!=0)
    {
        <h4>Найденные серии:</h4>
        <table class="table">
        @foreach (var item in Model)
        {
            <tr>
                <td class="col-md-4">@item.Name</td>
                <td>
                    @Html.ActionLink("Удалить", "Delete", new {id = item.Id}, new {@class = "col-md-2 col-md-offset-1"})
                </td>
                <td>
                    @Ajax.ActionLink("Редактировать", "Edit", "Series", new {id = @item.Id},
                        new AjaxOptions {UpdateTargetId = "editForm"}, new {@class = "col-md-2 col-md-offset-1"})
                </td>
            </tr>
        }
        </table>
    }
</div>